{
	"build": [{
		"name": "DSS",
		"component": ["buttonplate","flickerplate","navplate"],
		"sass": ["welcome.scss"],
		"js": ["src/welcome.js"]
	}],
	"project": {
		"component-first": ["jquery","hammerjs"],
		"component": ["navplate","flickerplate","modalplate","formplate"],
		"css": ["welcome.min.css","banner.css","dss.css","nav.css","portfolio.css","contacto.css","dsssection.css","contact-section.css"],
		"js": ["welcome.min.js","src/script.js","src/maps.js","src/events.js","src/renderer.js"],
		"ui": "fuse",
		"form-colour": "aqua",
		"icon-font": "font-awesome"
	},
	"build-watch": {
    	"js": ["src/*.js"],
    	"css": ["*.css"]
   	},
	"option": [
		"font-family: 'Roboto', 'Helvetic Neue', Helvetica, Arial"
	]
}